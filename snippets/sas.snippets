snippet head
	/*******************************************************************************
	* Filename: `Filename('$1.sas')`
	*
	* Author: `g:snips_author` | `g:snips_email` 
	*
	* Description: ${1}
	*
	*******************************************************************************/

	/* Libraries
	*/

snippet sort
	proc sort data=${1};
		by ${2};
		run;
	
snippet data
	data ${1};
		set ${2};
		${3}
		run;
	
snippet merge
	data ${1};
		merge ${2} (in=in1) ${3} (in=in2);
		by ${4};
		${5}
		run;
	
snippet make
	/* ${1} */
		* ${2:Description}
		*
		* INPUT: ${3}
		*
		* DATASET: ${4}
		*
		;
		%include '!MCLAB/${5}';
		${0}
	
snippet clean
	/* Clean up */
		proc datasets ;
			run; quit;
	
snippet trans
	proc transpose data=${1} out=${2};
		by ${3};
		var ${4};
		id ${5};
		run;
	
snippet freq
	proc freq data=${1};
		tables ${2};
		run;
	
snippet iter
	%iterdataset(dataset=${1}, function=%nrstr(%${2}(${3});));

snippet append
	proc append base=${1} data=${2};
	run;

snippet export
	proc export data=${1} outfile=${2} dbms=csv replace;
		putnames=yes;
		run;

snippet import
	proc import datafile=${1} out=${2} dbms=csv replace;
		getnames=yes;
		run;

